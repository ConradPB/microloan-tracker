{
    "version": 2,
    "builds": [
        {"src": "microloan_tracker/wsgi.py", "use": "@vercel/python"}
    ],
    "routes": [
        {"src": "/(.*)", "dest": "microloan_tracker/wsgi.py"}
    ],
    "env": {
        "DJANGO_SETTINGS_MODULE": "microloan_tracker.settings",
        "PYTHONPATH": "/vercel/path0"
    }
}